{
  "name": "@conlangtools/chronlang-engine",
  "version": "0.2.3",
  "exports": "./src/mod.ts",
  "imports": {
    "peggy": "npm:peggy@4.2.0"
  },
  "tasks": {
    "build": "deno task compile-grammar",
    "test": "deno task build && deno test -R",
    "bundle": "deno task build && npx vite build",
    "compile-grammar": "deno run --allow-read --allow-write compile-grammar.ts"
  }
}
